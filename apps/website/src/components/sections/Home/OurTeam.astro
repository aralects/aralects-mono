<script>
  import { cn } from "@repo/ui";
  import { smoothScrollTo } from "src/utils/newsletter";

  // State for selected dialects
  let selectedDialects: string[] = []; // Added type annotation

  // Get all dialect buttons
  const dialectButtons =
    document.querySelectorAll<HTMLButtonElement>(".dialect-button"); // Added type assertion

  // Function to handle dialect button clicks
  function handleDialectClick(
    dialect: string,
    buttonElement: HTMLButtonElement
  ) {
    // Added type annotations
    // Toggle selection
    if (selectedDialects.includes(dialect)) {
      selectedDialects = selectedDialects.filter((d) => d !== dialect);
    } else {
      selectedDialects = [...selectedDialects, dialect];
    }

    // Update button styles for all buttons
    updateButtonStyles();
  }

  // Function to update styles based on selection
  function updateButtonStyles() {
    dialectButtons.forEach((button: HTMLButtonElement) => {
      // Added type annotation
      const dialect = button.getAttribute("data-dialect");
      // Ensure dialect is not null before using it
      if (dialect === null) return;
      const isSelected = selectedDialects.includes(dialect);

      // Base classes (now applied directly to the button)
      const baseClasses =
        "px-3 py-1 md:px-6 md:py-2 font-SpaceGroteskBold rounded-full border border-white text-[16px] md:text-[22px] transition-colors text-white hover:bg-[#222222]";

      // Conditional classes (hover styles are applied directly here or via base hover)
      const conditionalClasses = isSelected
        ? "bg-[#252424] text-white hover:bg-[#252424]/80"
        : "hover:bg-[#222222] hover:text-[#fffff]";

      // Use cn to merge and apply classes
      // Make sure cn is available in the client-side scope or replace with manual class manipulation
      // Assuming cn is globally available or imported appropriately for client-side use
      // If not, replace the next line with manual classList add/remove logic
      button.className = cn(baseClasses, conditionalClasses);
    });
  }

  // Add event listeners to dialect buttons
  dialectButtons.forEach((button: HTMLButtonElement) => {
    // Added type annotation
    const dialect = button.getAttribute("data-dialect");
    if (dialect) {
      button.addEventListener("click", () =>
        handleDialectClick(dialect, button)
      );
    }
  });

  // Add event listener for the "Join waiting list" button
  const joinButton = document.getElementById(
    "join-waiting-list-button"
  ) as HTMLButtonElement | null; // Added type assertion
  if (joinButton) {
    // Ensure smoothScrollTo is correctly imported and available client-side
    joinButton.addEventListener("click", () =>
      smoothScrollTo("newsletter", 80)
    );
  }

  // Initial style update in case of pre-selected state (if needed later)
  // updateButtonStyles(); // Uncomment if initial state can be non-empty
</script>
